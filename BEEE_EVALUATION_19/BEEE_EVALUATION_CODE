int sensorPin = A0;
int ledPIN = 3;
int sensorValue = 0;
void setup() 
{
  Serial.begin(9600);
    pinMode(9,OUTPUT);
    pinMode(10,INPUT);  
      
}  
void loop() {
  int s
    s = analogRead(ldr port);
  if(s>300)
  {
    digitalWrite(9,HIGH);
    delay(1000);
    digitalWrite(9,LOW);
  }
  else
  { 
    
    digitalWrite(9,HIGH);
    delay(2000);
    digitalWrite(9,LOW);
  }  
}  
    
